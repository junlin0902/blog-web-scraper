{
  "name": "@redwoodjs/vite",
  "version": "8.4.0",
  "description": "Vite configuration package for Redwood",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/redwoodjs/redwood.git",
    "directory": "packages/vite"
  },
  "license": "MIT",
  "type": "module",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/cjs/index.d.ts",
        "default": "./dist/cjs/index.js"
      }
    },
    "./client": {
      "require": "./dist/cjs/client.js",
      "import": "./dist/client.js"
    },
    "./buildFeServer": {
      "require": "./dist/cjs/buildFeServer.js",
      "import": "./dist/buildFeServer.js"
    },
    "./build": {
      "import": "./dist/build/build.js",
      "default": "./dist/cjs/build/build.js"
    },
    "./bins/rw-vite-build.mjs": "./bins/rw-vite-build.mjs"
  },
  "bin": {
    "rw-dev-fe": "./dist/devFeServer.js",
    "rw-serve-fe": "./dist/runFeServer.js",
    "rw-vite-build": "./bins/rw-vite-build.mjs",
    "rw-vite-dev": "./bins/rw-vite-dev.mjs",
    "vite": "./bins/vite.mjs"
  },
  "files": [
    "dist",
    "inject"
  ],
  "scripts": {
    "build": "tsx build.ts && yarn build:types",
    "build:pack": "yarn pack -o redwoodjs-vite.tgz",
    "build:types": "tsc --build --verbose ./tsconfig.build.json",
    "check:attw": "tsx ./attw.ts",
    "check:package": "concurrently npm:check:attw yarn:publint",
    "test": "vitest run",
    "test:watch": "vitest watch"
  },
  "dependencies": {
    "@babel/generator": "7.25.7",
    "@babel/parser": "^7.22.16",
    "@babel/traverse": "^7.22.20",
    "@redwoodjs/auth": "8.4.0",
    "@redwoodjs/babel-config": "8.4.0",
    "@redwoodjs/cookie-jar": "8.4.0",
    "@redwoodjs/internal": "8.4.0",
    "@redwoodjs/project-config": "8.4.0",
    "@redwoodjs/server-store": "8.4.0",
    "@redwoodjs/web": "8.4.0",
    "@swc/core": "1.7.28",
    "@vitejs/plugin-react": "4.3.2",
    "@whatwg-node/fetch": "0.9.21",
    "@whatwg-node/server": "0.9.49",
    "acorn-loose": "8.4.0",
    "buffer": "6.0.3",
    "busboy": "^1.6.0",
    "cookie": "0.7.2",
    "core-js": "3.38.1",
    "dotenv-defaults": "5.0.2",
    "execa": "5.1.1",
    "express": "4.21.0",
    "find-my-way": "8.2.2",
    "fs-extra": "11.2.0",
    "http-proxy-middleware": "3.0.3",
    "isbot": "5.1.17",
    "react": "18.3.1",
    "react-server-dom-webpack": "19.0.0-rc-8269d55d-20240802",
    "rimraf": "6.0.1",
    "vite": "5.4.8",
    "vite-plugin-cjs-interop": "2.1.4",
    "vite-plugin-node-polyfills": "0.22.0",
    "ws": "8.18.0",
    "yargs-parser": "21.1.1"
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "0.16.4",
    "@hyrious/esbuild-plugin-commonjs": "0.2.4",
    "@types/busboy": "^1",
    "@types/cookie": "^0",
    "@types/express": "4",
    "@types/fs-extra": "11.0.4",
    "@types/react": "^18.2.55",
    "@types/ws": "^8",
    "@types/yargs-parser": "21.0.3",
    "concurrently": "8.2.2",
    "glob": "11.0.0",
    "memfs": "4.13.0",
    "publint": "0.2.11",
    "rollup": "4.24.0",
    "tsx": "4.19.1",
    "typescript": "5.6.2",
    "vitest": "2.0.5"
  },
  "gitHead": "c2128f0c6c1ee82fe54aa0dd8bac269167b84485"
}
